{
  "name": "ouroboros",
  "version": "0.1.0",
  "main": "dist/main/electron.prod.js",
  "repository": "git@github.com:gsxhnd/ouroboros.git",
  "author": "gsxhnd <gsxhnd@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/main/electron.js",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "start:render": "NODE_ENV=development webpack serve --config ./webpack/webpack.render.dev.js --open",
    "build:render": "NODE_ENV=production webpack --config ./webpack/webpack.render.prod.js",
    "package:mac": "electron-builder build --mac"
  },
  "build": {
    "productName": "Electron App",
    "appId": "electron.app",
    "asar": true,
    "files": [
      "dist"
    ],
    "extends": null,
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "dmg": {
      "backgroundColor": "#ffffff",
      "title": "Electron App"
    },
    "directories": {
      "output": "release"
    }
  },
  "devDependencies": {
    "@fluentui/react": "^8.26.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "electron": "^13.1.8",
    "electron-builder": "^22.11.7",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "web-vitals": "^1.0.1",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
