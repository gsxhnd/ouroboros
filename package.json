{
  "name": "ouroboros",
  "version": "0.1.0",
  "main": "dist/main/index.js",
  "type": "module",
  "repository": "git@github.com:gsxhnd/ouroboros.git",
  "author": "gsxhnd <gsxhnd@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:main": "tsc --build --clean && tsc --build --verbose",
    "start:electron": "electron .",
    "start:render": "webpack serve --config ./webpack/webpack.render.dev.js",
    "build:render": "webpack --config ./webpack/webpack.render.prod.js",
    "package:mac": "electron-builder build --mac",
    "package:win": "electron-builder build --win"
  },
  "build": {
    "productName": "Ouroboros",
    "appId": "xyz.gsxhnd.ouroboros",
    "asar": false,
    "files": [
      "dist"
    ],
    "extends": null,
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./resources/logo_512.png",
      "target": "dir"
    },
    "directories": {
      "output": "release"
    }
  },
  "devDependencies": {
    "@fluentui/react": "^8.36.5",
    "@types/node": "^16.10.9",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "electron-store": "^8.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "lowdb": "^3.0.0",
    "mini-css-extract-plugin": "^2.4.2",
    "monaco-editor": "^0.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {}
}
